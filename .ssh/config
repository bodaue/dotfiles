# VPS сервер
Host vpn
    HostName 185.114.116.74
    User root
    Port 9749
    IdentityFile ~/.ssh/vpn
    ServerAliveInterval 60
    ServerAliveCountMax 3

Host git.kts.tech
    HostName git.kts.tech
    User git
    IdentityFile ~/.ssh/id_gitlab
    IdentitiesOnly yes

Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_github
    IdentitiesOnly yes
